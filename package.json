{
  "name": "vue-form-error",
  "description": "A Vue.js component that is used to display validation errors on form inputs.",
  "author": "Ziad Adeela <zeiad.suliman@gmail.com> (https://github.com/ziadadeela)",
  "version": "1.0.0",
  "main": "./dist/FormError.common.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ziadadeela/vue-form-error"
  },
  "bugs": {
    "url": "https://github.com/ziadadeela/vue-form-error/issues"
  },
  "homepage": "https://github.com/ziadadeela/vue-form-error#readme",
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "test": "vue-cli-service test",
    "build": "vue-cli-service build",
